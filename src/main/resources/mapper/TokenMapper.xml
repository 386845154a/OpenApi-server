<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.hollykunge.openapi.mapper.TokenMapper">
   <select id = "getSelfToken" resultType="string">
       select token  from api_token where app_id  = #{appId}
   </select>
</mapper>